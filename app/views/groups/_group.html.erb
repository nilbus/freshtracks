<tr id="group<%= group.id %>main" class="main">
  <td class="expander"><a href="javascript:void(0)" onclick="this.innerHTML = toggle_row('group<%=group.id%>')">+</a></td>
  <td>GRP <%=group.number%></td>
  <td colspan="2"><%= group.name%></td>
</tr>
<tr id="group<%= group.id %>extra" class="extra">
<td colspan="4">
  <div class="slider">
    <%= render :partial => 'courses/course_list', :object => group.group_members.map(&:course) -%>
  </div>
</td></tr>
