<%= javascript_tag do %>
  <%- if created.new_record? -%>
    var error_message = 'This section <%= (created.errors.any? ? created.errors.values.join(' ') : "couldn't be added." )%>';
    appendError("#section-<%= @wishlist_section.section.id %> .button", error_message);
  <%- end -%>
  disableWishlistButton(<%=created.section_id%>);
<% end -%>
