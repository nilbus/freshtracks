<%- unique_id = generate_unique_id completed_req.id -%>
<div class="<%= completed_req.id %>">
  <div class="row main" id="<%= unique_id %>main" uid="<%=unique_id%>">
    <td><%= completed_req.course.name %></td> 
    <td>Grade: <%= completed_req.grade %></td>
  </div>

  <div class="row extra" id="<%=unique_id%>extra" uid="<%=unique_id%>">
    <td colspan="3">
      <div class="slider" style="width: 80%; margin-left: 1em ;">
        <div style="width: 50%; float: left">Fulfills: <%= completed_req.degree_requirement.required.name %> </div>
        <div style="width: 50%; float: left"><%=completed_req.semester.name%></div>
      </div>
    </td>
  </div>
</div><!-- # -->
